        </activity>
        <activity android:name=".options_activity"/>
        <activity android:name=".Authenticiation.LoginActivity"></activity>
        <activity android:name=".restaurant_home_page"></activity>
        <activity android:name=".item_counter_restaurant"></activity>
        <activity android:name=".restaurant_itemselection_page"></activity>
        <activity android:name=".parking_details_page_activity"></activity>
